# ==========================
# Slack通知設定
# ==========================
# SlackのBot Token（GitHub Secretsで管理）
SLACK_TOKEN=

# Slackの通知チャンネル（例: #ebay-auto-research）
SLACK_CHANNEL=#ebay-auto-research


# ==========================
# Supabase接続設定
# ==========================
# SupabaseのURL
SUPABASE_URL=

# SupabaseのAPIキー（anon keyまたはservice_role key）
SUPABASE_KEY=


# ==========================
# eBay API（またはスクレイピング）設定
# ==========================
# eBay App ID
EBAY_APP_ID=

# eBay Cert ID
EBAY_CERT_ID=

# eBay Dev ID
EBAY_DEV_ID=

# eBay Auth Token
EBAY_AUTH_TOKEN=


# ==========================
# その他（必要であれば）
# ==========================
# デバッグモード（True/False）
DEBUG_MODE=False
